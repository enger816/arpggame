{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Chrome against localhost, with sourcemaps",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost/arpg/",
            //"url": "http://h5.g2.cn/joinGame/code/5/gid/5/",
            "sourceMaps": true,
            //"userDataDir": "${workspaceRoot}/.vscode/chrome",
            "diagnosticLogging": false,
            "webRoot": "${workspaceRoot}"
        },
        {
            "name": "Attach to Chrome, with sourcemaps",
            "type": "chrome",
            "request": "attach",
            "port": 9222,
            "sourceMaps": true,
            "webRoot": "${workspaceRoot}"
        }
    ]
}